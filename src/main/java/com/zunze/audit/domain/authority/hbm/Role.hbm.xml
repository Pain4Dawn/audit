<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="zunze.audit.domain.authority">	
	<class name="Role" table="T_ROLE">
		<id name="id" column="ID">
			<generator class="identity"/>
		</id>
		
		<property name="roleName" column="ROLENAME"></property>
		<property name="roleType" column="ROLETYPE"></property>
		<property name="province" column="PROVINCE"></property>
		<property name="startDate" column="STARTDATE"></property>
		<property name="endDate" column="ENDDATE"></property>
		
		<set
			inverse="true"
			name="childRoles">
			<key column="PARENTID"/>
			<one-to-many class="Role"/>
		</set>
		
		<set 
			name="userSet"
			inverse="true"
			table="T_USER_ROLE"
			>
			<key column="ROLEID"/>
			<many-to-many 
				class="USER"
				column="id"/>	
		</set>
	</class>
</hibernate-mapping>