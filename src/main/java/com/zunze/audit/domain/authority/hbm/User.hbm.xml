<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="zunze.audit.domain.authority">
	<class name="User" table="T_USER">
		<id name="id" column="ID" type="integer">
			<generator class="identity"/>
		</id>
		<property name="userName" column="USERNAME" type="String"></property>
		<many-to-one 
			name="provinceInfo"
			class="zunze.audit.domain.ProvinceInfo"
			column="PROVINCEINFO_ID">
		</many-to-one>
		
		<set name="roleSet"
			 table="T_USER_ROLE"
			 inverse="true"
			 >
			<key column="USERID"/>
			<many-to-many 
				class="Role"
				column="id"/>
		</set>
		
		<set name="menuSet"
			table="T_USER_MENU"
			inverse="true">
				<key column="USERID"></key>
				<many-to-many
					class="Menu"
					column="id"/>
			</set>
	</class>
</hibernate-mapping>